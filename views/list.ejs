<%- include("header") -%>

<div class="box" id="heading">
  <h1>It's a <%= listTitle %>!</h1>
</div>

<div class="box">
  <% for(task in items){ %>
  <div class="item">
    <input type="checkbox" />
    <p><%= items[task] %></p>
  </div>
  <% } %>

  <form class="item" action="/" method="post">
    <input type="text" name="task" placeholder="New Item" autocomplete="off" />
    <button type="submit" name="list" value="<%= listTitle %>">+</button>
  </form>
</div>

<%- include("footer") -%>
